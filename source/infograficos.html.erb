---
title: Infográficos
---

<section class="post <%= page_classes("infograficos") %>">
  <header>
    <div class="content">
      <h3><%= current_page.data.title %></h3>
    </div>
  </header>

  <div class="content">
    <%= partial "/partials/filter_form" %>
  </div>

  <article class="<%= current_page.data.title.parameterize %>">
    <%= partial "partials/inphographics-list" %>

    <footer>
      <%= link_to "#{fa_icon 'chevron-down'} Mostrar mais infográficos #{fa_icon 'chevron-down'}", "" %>
    </footer>
  </article>
</section>
